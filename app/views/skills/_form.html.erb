<%= render "layouts/errors", :object => @skill %>

<div id="container">
  <%= form_for(@skill) do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <%= f.label :description %>
    <%= f.text_field :description %>

    <%= f.submit :class => "btn btn-sm btn-sucess"%>
  <% end %>
</div>
