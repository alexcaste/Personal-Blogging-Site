<%= render "layouts/errors", :object => @skill %>

<div id="container">
  <div id="row ">
    <form class="form-horizontal" role="form">
      <div class="form-group">
        <%= form_for(@skill) do |f| %>
          <%= f.label :name %>
          <%= f.text_field :name %>
          <%= f.label :description %>
          <%= f.text_field :description %>

          <%= f.submit :class => "btn btn-sucess"%>
        <% end %>
      </div>
    </form>
  </div>
</div>
